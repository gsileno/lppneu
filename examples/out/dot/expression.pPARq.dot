digraph G {
  rankdir="LR";
  subgraph place {
    graph [shape=circle,color=gray];
    node [shape=circle,fixedsize=true,width=.5];
    pl0 [label="PAR(p, q)"];
    pl1 [label="p"];
    pl2 [label="q"];
  } 
  subgraph transitions {
    node [shape=rect,height=.5,width=.5];
    tr0 [label="null"];
    tr1 [label="p"];
    tr2 [label="q"];
  } 
  tr1 -> pl1 [] ;
  tr0 -> pl1 [] ;
  pl1 -> tr0 [] ;
  tr2 -> pl2 [] ;
  tr0 -> pl2 [] ;
  pl2 -> tr0 [] ;
  tr0 -> pl0 [] ;
  pl0 -> tr0 [arrowhead=dot] ;
}

